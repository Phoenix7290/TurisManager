@page "{id:int}"
@model TurisManager.Pages.Clientes.Detalhes.DetalhesModel
@{
	ViewData["Title"] = "Detalhes do Cliente";
}

<h1>Detalhes do Cliente</h1>
<div>
    <dl class="row">
        <dt class="col-sm-2">Nome</dt>
        <dd class="col-sm-10">@Model.Cliente.Nome</dd>
        <dt class="col-sm-2">Email</dt>
        <dd class="col-sm-10">@Model.Cliente.Email</dd>
    </dl>
</div>
<a asp-page="./Index">Voltar</a>